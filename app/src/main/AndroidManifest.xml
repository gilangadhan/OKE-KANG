<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.veronica.sfpcc.okekang">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SayaActivity"
            android:label="@string/title_activity_saya"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NotifikasiActivity"
            android:label="@string/title_activity_notifikasi"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LainnyaActivity"
            android:label="@string/title_activity_lainnya"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Content_Main"
            android:label="@string/title_activity_content_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TukangACActivity"></activity>
    </application>

</manifest>